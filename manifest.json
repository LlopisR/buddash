{
  "name": "Buddash",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Visual todo app for websites in your new tab",
  "homepage_url": "http://www.heavenlabs.com",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  }
}